@model IEnumerable<Proyecto_Inventarios.Models.Bodegas_Inventarios.Bjuguetes>

@{
    ViewBag.Title = "Index";
}

<h2>Bodega juguetes</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tipo de producto
        </th>
        <th>
            Cantidad existente
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.tipoproducto)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.cantexistente)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.tipoproducto }) |
        @Html.ActionLink("Detalles", "Details", new { id = item.tipoproducto }) |
        @Html.ActionLink("Eliminar", "Delete", new { id = item.tipoproducto })
    </td>
</tr>
}

</table>
<p><a href="/PrimeraClasses/index" class="btn btn-primary btn-lg" style="background-color:#F1C80D">Volver al menu de inventarios &raquo;</a></p>
